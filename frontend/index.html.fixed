        // ============================================
        // COMPLETE IRIDOLOGY MODULE COMPONENT
        // ============================================
        // NEW IRIDOLOGY MODULE
const IridologyNew = () => {
    const [currentStep, setCurrentStep] = React.useState('select-patient');
    const [selectedPatient, setSelectedPatient] = React.useState(null);
    const [patients, setPatients] = React.useState([]);
    const [analysisId, setAnalysisId] = React.useState(null);
    const [leftEyeImage, setLeftEyeImage] = React.useState(null);
    const [rightEyeImage, setRightEyeImage] = React.useState(null);
    const [captureMethod, setCaptureMethod] = React.useState('upload');
    const [analysisResults, setAnalysisResults] = React.useState(null);
    const [loading, setLoading] = React.useState(false);
    const [error, setError] = React.useState(null);
    const [recentAnalyses, setRecentAnalyses] = React.useState([]);
    const [analysisProgress, setAnalysisProgress] = React.useState(0);
    const [analysisStage, setAnalysisStage] = React.useState('');
    const [searchTerm, setSearchTerm] = React.useState("");

    React.useEffect(() => {
        loadPatients();
        loadRecentAnalyses();
    }, []);

    const loadPatients = async () => {
        try {
            const response = await fetch('/api/v1/clinic/patients', {
                headers: {
                    'Authorization': `Bearer ${localStorage.getItem('celloxen_token')}`
                }
            });
            const data = await response.json();
            setPatients(data || []);
        } catch (err) {
            console.error('Error loading patients:', err);
        }
    };

    const loadRecentAnalyses = async () => {
        try {
            const response = await fetch('/api/v1/iridology/recent?limit=5', {
                headers: {
                    'Authorization': `Bearer ${localStorage.getItem('celloxen_token')}`
                }
            });
            if (response.ok) {
                const data = await response.json();
                setRecentAnalyses(data.analyses || []);
            }
        } catch (err) {
            console.error('Error loading recent analyses:', err);
        }
    };

    const startNewAnalysis = async () => {
        if (!selectedPatient) {
            setError('Please select a patient');
            return;
        }

        setLoading(true);
        setError(null);

        try {
            const response = await fetch(`/api/v1/iridology/start?patient_id=${selectedPatient.id}&disclaimer_accepted=true`, {
                method: 'POST',
                headers: {
                    'Authorization': `Bearer ${localStorage.getItem('celloxen_token')}`
                }
            });

            const data = await response.json();

            if (response.ok && data.success) {
                setAnalysisId(data.analysis_id);
                setCurrentStep('upload-images');
            } else {
                setError(data.detail || 'Failed to start analysis');
            }
        } catch (err) {
            setError('Error starting analysis: ' + err.message);
        } finally {
            setLoading(false);
        }
    };

    const handleImageUpload = (eye, event) => {
        const file = event.target.files[0];
        if (!file) return;

        const reader = new FileReader();
        reader.onload = (e) => {
            if (eye === 'left') {
                setLeftEyeImage(e.target.result);
            } else {
                setRightEyeImage(e.target.result);
            }
        };
        reader.readAsDataURL(file);
    };

    const uploadImages = async () => {
        if (!leftEyeImage || !rightEyeImage) {
            setError('Please upload both eye images');
            return;
        }

        setLoading(true);
        setError(null);

        try {
            const response = await fetch(`/api/v1/iridology/${analysisId}/upload-images`, {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/json',
                    'Authorization': `Bearer ${localStorage.getItem('celloxen_token')}`
                },
                body: JSON.stringify({
                    left_eye_image: leftEyeImage,
                    right_eye_image: rightEyeImage,
                    capture_method: captureMethod
                })
            });

            const data = await response.json();

            if (response.ok && data.success) {
                setCurrentStep('ready-to-analyse');
            } else {
                setError(data.detail || 'Failed to upload images');
            }
        } catch (err) {
            setError('Error uploading images: ' + err.message);
        } finally {
            setLoading(false);
        }
    };

    const runAnalysis = async () => {
        setLoading(true);
        setError(null);
        setCurrentStep('analysing');
        setAnalysisProgress(0);
        setAnalysisStage('Initializing AI analysis...');

        try {
            const progressStages = [
                { progress: 10, stage: 'Connecting to AI system...', delay: 500 },
                { progress: 25, stage: 'Analyzing left eye image...', delay: 8000 },
                { progress: 50, stage: 'Analyzing right eye image...', delay: 8000 },
                { progress: 75, stage: 'Synthesizing bilateral analysis...', delay: 8000 },
                { progress: 90, stage: 'Generating wellness report...', delay: 5000 },
            ];

            const apiPromise = fetch(`/api/v1/iridology/${analysisId}/analyse`, {
                method: 'POST',
                headers: {
                    'Authorization': `Bearer ${localStorage.getItem('celloxen_token')}`
                }
            });

            for (const stage of progressStages) {
                await new Promise(resolve => setTimeout(resolve, stage.delay));
                setAnalysisProgress(stage.progress);
                setAnalysisStage(stage.stage);
            }

            const response = await apiPromise;
            const data = await response.json();

            setAnalysisProgress(100);
            setAnalysisStage('Analysis complete!');

            if (response.ok && data.success) {
                const resultsResponse = await fetch(`/api/v1/iridology/${analysisId}/results`, {
                    headers: {
                        'Authorization': `Bearer ${localStorage.getItem('celloxen_token')}`
                    }
                });

                const resultsData = await resultsResponse.json();
                setAnalysisResults(resultsData);
                
                setTimeout(() => {
                    setCurrentStep('view-results');
                    loadRecentAnalyses();
                }, 1000);
            } else {
                setError(data.detail || 'Analysis failed');
                setCurrentStep('ready-to-analyse');
            }
        } catch (err) {
            setError('Error running analysis: ' + err.message);
            setCurrentStep('ready-to-analyse');
        } finally {
            setLoading(false);
        }
    };

    const viewReport = () => {
        window.open(`/iridology_report.html?analysisId=${analysisId}`, '_blank');
    };

    const downloadReport = async () => {
        try {
            const response = await fetch(`/api/v1/iridology/${analysisId}/download-pdf`, {
                headers: {
                    'Authorization': `Bearer ${localStorage.getItem('celloxen_token')}`
                }
            });

            if (response.ok) {
                const blob = await response.blob();
                const url = window.URL.createObjectURL(blob);
                const a = document.createElement('a');
                a.href = url;
                a.download = `Iridology_Report_${analysisResults?.analysis?.analysis_number}.pdf`;
                document.body.appendChild(a);
                a.click();
                window.URL.revokeObjectURL(url);
                document.body.removeChild(a);
            } else {
                alert('PDF download is currently unavailable. Please use "View Full Report" instead.');
            }
        } catch (err) {
            alert('Error downloading report. Please use "View Full Report" instead.');
        }
    };

    const resetAnalysis = () => {
        setCurrentStep('select-patient');
        setSelectedPatient(null);
        setAnalysisId(null);
        setLeftEyeImage(null);
        setRightEyeImage(null);
        setAnalysisResults(null);
        setError(null);
        setAnalysisProgress(0);
        setAnalysisStage('');
        loadRecentAnalyses();
    };

