<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Celloxen Health Portal</title>
    <script src="https://unpkg.com/react@18/umd/react.production.min.js">
// Fetch and display patient data for iridology analysis

// Helper function to extract constitutional data from combined_analysis
function extractConstitutionalData(combinedAnalysis) {
    try {
        if (!combinedAnalysis || !combinedAnalysis.raw_text) {
            return { type: null, strength: null, analysisDate: null };
        }
        
        const text = combinedAnalysis.raw_text;
        let constitutionalType = null;
        let constitutionalStrength = null;
        
        // Extract from "Left Eye: [Type] Constitution ([Strength])" and "Right Eye: [Type] Constitution ([Strength])"
        const leftMatch = text.match(/Left Eye:\s*([^C]+)Constitution\s*\(([^)]+)\)/i);
        const rightMatch = text.match(/Right Eye:\s*([^C]+)Constitution\s*\(([^)]+)\)/i);
        
        if (leftMatch && rightMatch) {
            const leftType = leftMatch[1].trim();
            const rightType = rightMatch[1].trim();
            const leftStrength = leftMatch[2].trim();
            const rightStrength = rightMatch[2].trim();
            
            // Combine the types
            if (leftType === rightType) {
                constitutionalType = leftType;
            } else {
                constitutionalType = `${leftType}/${rightType}`;
            }
            
            // Use the strength (they should be the same or similar)
            constitutionalStrength = leftStrength;
        }
        
        return { 
            type: constitutionalType, 
            strength: constitutionalStrength 
        };
    } catch (error) {
        console.error('Error extracting constitutional data:', error);
        return { type: null, strength: null };
    }
}


async function displayIridologyWithPatient(analysis) {
    console.log('üìä Displaying iridology analysis:', analysis);
    currentAnalysis = analysis;
    
    // Show the analysis result section
    const resultSection = document.getElementById('analysisResult');
    if (resultSection) {
        resultSection.style.display = 'block';
    }
    
    // Fetch patient data
    try {
        const patientResponse = await fetch(`/api/v1/patients/${analysis.patient_id}`);
        if (patientResponse.ok) {
            const patient = await patientResponse.json();
            console.log('‚úÖ Patient data loaded:', patient);
            
            // Update patient name and number
            const nameElements = document.querySelectorAll('[id*="patientName"], .patient-name');
            nameElements.forEach(el => {
                if (el) el.textContent = `${patient.first_name} ${patient.last_name}`;
            });
            
            const numberElements = document.querySelectorAll('[id*="patientNumber"], .patient-number');
            numberElements.forEach(el => {
                if (el) el.textContent = patient.patient_number;
            });
        }
    } catch (error) {
        console.error('Error fetching patient:', error);
    }
    
    // Update analysis details
    const analysisNumberEl = document.getElementById('analysisNumber');
    if (analysisNumberEl) analysisNumberEl.textContent = analysis.analysis_number || 'N/A';
    
    const constitutionalTypeEl = document.getElementById('constitutionalType');
    if (constitutionalTypeEl) constitutionalTypeEl.textContent = analysis.constitutional_type || 'Unknown';
    
    const strengthEl = document.getElementById('organStrength') || document.getElementById('constitutionalStrength');
    if (strengthEl) strengthEl.textContent = analysis.constitutional_strength || 'Unknown';
}

    </script>
    <script src="https://unpkg.com/react-dom@18/umd/react-dom.production.min.js"></script>
    <script src="https://unpkg.com/@babel/standalone/babel.min.js"></script>
