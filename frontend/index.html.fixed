        // ============================================
        const ImageCaptureStep = ({ captureMethod, setCaptureMethod, leftEyeImage, rightEyeImage, onImageCapture, onStartAnalysis, onBack }) => {
            const leftFileInputRef = React.useRef(null);
            const rightFileInputRef = React.useRef(null);

            const handleFileUpload = (eye, event) => {
                const file = event.target.files[0];
                if (file) {
                    const reader = new FileReader();
                    reader.onload = (e) => {
                        onImageCapture(eye, e.target.result);
                    };
                    reader.readAsDataURL(file);
