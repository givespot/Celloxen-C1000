                    {step === 2 && (
                        <DisclaimerStep
                            patient={selectedPatient}
                            disclaimerAccepted={disclaimerAccepted}
                            setDisclaimerAccepted={setDisclaimerAccepted}
                            onContinue={handleDisclaimerContinue}
                            onBack={() => setStep(1)}
                            loading={loading}
