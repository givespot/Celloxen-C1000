const IridologyModule = () => {
            const [step, setStep] = React.useState(1); // 1=patient, 2=disclaimer, 3=capture, 4=analysis, 5=results
            const [selectedPatient, setSelectedPatient] = React.useState(null);
            const [searchTerm, setSearchTerm] = React.useState('');
            const [patients, setPatients] = React.useState([]);
            const [disclaimerAccepted, setDisclaimerAccepted] = React.useState({
                readToPatient: false,
                patientAccepts: false
            });
            const [captureMethod, setCaptureMethod] = React.useState(null); // 'camera' or 'upload'
            const [leftEyeImage, setLeftEyeImage] = React.useState(null);
            const [rightEyeImage, setRightEyeImage] = React.useState(null);
            const [analysisId, setAnalysisId] = React.useState(null);
            const [analysisResults, setAnalysisResults] = React.useState(null);
            const [loading, setLoading] = React.useState(false);
            const [error, setError] = React.useState(null);

            // Load patients on mount
            React.useEffect(() => {
                loadPatients();
            }, []);

            const loadPatients = async () => {
                try {
                    const response = await fetch('/api/v1/clinic/patients');
                    const data = await response.json();
                    setPatients(Array.isArray(data) ? data.filter(p => p.status === 'active') : []);
                } catch (err) {
                    console.error('Error loading patients:', err);
                }
            };

            const filteredPatients = patients.filter(p => 
                `${p.first_name} ${p.last_name} ${p.patient_number}`
                    .toLowerCase()
                    .includes(searchTerm.toLowerCase())
            );

            const handlePatientSelect = (patient) => {
                setSelectedPatient(patient);
                setStep(2);
            };

            const handleDisclaimerContinue = async () => {
                if (!disclaimerAccepted.readToPatient || !disclaimerAccepted.patientAccepts) {
                    setError('Both checkboxes must be ticked to continue');
                    return;
                }

                setLoading(true);
                setError(null);

                try {
                    const response = await fetch(
                        `/api/v1/iridology/start?patient_id=${selectedPatient.id}&disclaimer_accepted=true`,
                        { method: 'POST' }
                    );
                    
                    const data = await response.json();
                    
                    if (data.success) {
                        setAnalysisId(data.analysis_id);
                        setStep(3);
                    } else {
                        setError(data.detail || 'Failed to start analysis');
                    }
                } catch (err) {
                    setError('Network error: ' + err.message);
                } finally {
                    setLoading(false);
                }
            };

            const handleImageCapture = (eye, imageData) => {
                if (eye === 'left') {
                    setLeftEyeImage(imageData);
                } else {
                    setRightEyeImage(imageData);
                }
            };

            const handleStartAnalysis = async () => {
                if (!leftEyeImage || !rightEyeImage) {
                    setError('Please capture both eye images');
                    return;
                }

                setLoading(true);
                setError(null);
                setStep(4); // Move to analysis progress screen

